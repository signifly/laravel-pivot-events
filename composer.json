{
    "name": "signifly/laravel-pivot-events",
    "description": "Trigger Eloquent model pivot events when attaching, detaching or updating.",
    "homepage": "https://github.com/signifly/laravel-pivot-events",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.6",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\PivotEvents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\PivotEvents\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": []
        }
    }
}
